<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>osx.environment</string>
  <key>ProgramArguments</key>
  <!-- PATH does not work, bug at apple side -->
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>
        BASH_NO=no . /etc/profile
    </string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardErrorPath</key>
  <string>/tmp/osx.environment.stderr</string>
  <key>StandardOutPath</key>
  <string>/tmp/osx.environment.stdout</string>
</dict>
</plist>
