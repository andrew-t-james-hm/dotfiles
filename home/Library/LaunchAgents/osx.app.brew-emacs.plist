<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<true/>
	<key>Label</key>
	<string>homebrew.mxcl.emacs</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>-c</string>
		<string>BASH_NO=no . /etc/profile &amp;&amp; /usr/local/bin/emacs --daemon</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StandardErrorPath</key>
	<string>/tmp/homebrew.mxcl.emacs.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/homebrew.mxcl.emacs.stdout</string>
</dict>
</plist>
